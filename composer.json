{
	"name": "domjudge/domjudge",
	"description": "DOMjudge Programming Contest Control System",
	"homepage": "https://www.domjudge.org",
	"license": "GPL-2.0+",
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "fortawesome/font-awesome",
				"version": "6.2.1",
				"dist": {
					"url": "https://github.com/FortAwesome/Font-Awesome/releases/download/6.2.1/fontawesome-free-6.2.1-web.zip",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "angel-vladov/select2-theme-bootstrap4",
				"version": "1.0.2",
				"dist": {
					"url": "https://github.com/angel-vladov/select2-theme-bootstrap4/archive/1.0.2.zip",
					"type": "zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "eligrey/filesaver",
				"version": "2.0.4",
				"dist": {
					"url": "https://github.com/eligrey/FileSaver.js/archive/v2.0.4.zip",
					"type": "zip"
				}
			}
		}
	],
	"require": {
		"php": "^7.4.0",
		"ext-ctype": "*",
		"ext-curl": "*",
		"ext-fileinfo": "*",
		"ext-gd": "*",
		"ext-iconv": "*",
		"ext-intl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-mysqli": "*",
		"ext-pcntl": "*",
		"ext-posix": "*",
		"ext-simplexml": "*",
		"ext-zip": "*",
		"angel-vladov/select2-theme-bootstrap4": "1.0.2",
		"components/flag-icon-css": "^3.5",
		"components/jquery": "^3.6",
		"datatables/datatables": "^1.10",
		"doctrine/doctrine-bundle": "^2.8",
		"doctrine/doctrine-fixtures-bundle": "^3.4",
		"doctrine/doctrine-migrations-bundle": "^3.2",
		"doctrine/orm": "^2.14",
		"eligrey/filesaver": "2.*",
		"endclothing/prometheus_client_php": "^1.0",
		"fortawesome/font-awesome": "6.*",
		"friendsofsymfony/rest-bundle": "^3.5",
		"ircmaxell/password-compat": "*",
		"jms/serializer-bundle": "^5.2",
		"league/commonmark": "^2.3",
		"mbostock/d3": "^3.5",
		"nelmio/api-doc-bundle": "^4.11",
		"novus/nvd3": "^1.8",
		"phpunit/phpunit": "^9.6",
		"ramsey/uuid": "^4.2",
		"sebastian/diff": "^4.0",
		"select2/select2": "4.*",
		"sensio/framework-extra-bundle": "^6.2",
		"sentry/sentry-symfony": "^4.5",
		"symfony/asset": "5.4.*",
		"symfony/browser-kit": "5.4.*",
		"symfony/console": "5.4.*",
		"symfony/css-selector": "5.4.*",
		"symfony/dotenv": "5.4.*",
		"symfony/expression-language": "5.4.*",
		"symfony/flex": "^1",
		"symfony/form": "5.4.*",
		"symfony/framework-bundle": "5.4.*",
		"symfony/http-client": "5.4.*",
		"symfony/intl": "5.4.*",
		"symfony/mime": "5.4.*",
		"symfony/monolog-bundle": "^3.8.0",
		"symfony/security-bundle": "5.4.*",
		"symfony/security-csrf": "5.4.*",
		"symfony/stopwatch": "5.4.*",
		"symfony/twig-bundle": "5.4.*",
		"symfony/validator": "5.4.*",
		"symfony/web-profiler-bundle": "5.4.*",
		"symfony/yaml": "5.4.*",
		"twbs/bootstrap": "^4.6.0",
		"twig/extra-bundle": "^3.5",
		"twig/markdown-extra": "^3.5",
		"twig/string-extra": "^3.5",
		"twig/twig": "^3.5"
	},
	"require-dev": {
		"ext-dom": "*",
		"dama/doctrine-test-bundle": "^7.0",
		"squizlabs/php_codesniffer": "*",
		"symfony/debug-bundle": "5.4.*",
		"symfony/maker-bundle": "^1.42",
		"symfony/phpunit-bridge": "6.0.*",
		"symfony/var-dumper": "5.4.*"
	},
	"autoload": {
		"psr-4": {
			"App\\": "webapp/src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "webapp/tests/"
		}
	},
	"config": {
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true,
		"platform": {
			"php": "7.4.0"
		},
		"vendor-dir": "lib/vendor",
		"component-dir": "lib/vendor/components",
		"allow-plugins": {
			"composer/package-versions-deprecated": true,
			"symfony/flex": true
		}
	},
	"replace": {
		"paragonie/random_compat": "2.*",
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-iconv": "*",
		"symfony/polyfill-php71": "*",
		"symfony/polyfill-php70": "*",
		"symfony/polyfill-php56": "*"
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd"
		},
		"post-install-cmd": [
			"@auto-scripts"
		],
		"post-update-cmd": [
			"@auto-scripts"
		]
	},
	"conflict": {
		"symfony/symfony": "*"
	},
	"extra": {
		"symfony": {
			"root-dir": "webapp/",
			"allow-contrib": true,
			"require": "5.4.*"
		}
	}
}
