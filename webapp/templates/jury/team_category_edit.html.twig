{% extends "jury/base.html.twig" %}
{% import "jury/jury_macros.twig" as macros %}

{% block title %}Edit category {{ teamCategory.externalid }} - {{ parent() }}{% endblock %}

{% block extrahead %}
    {{ parent() }}
    {{ macros.coloris_extrahead() }}
    {{ macros.select2_extrahead() }}
    {{ macros.toggle_extrahead() }}
{% endblock %}

{% block content %}

    <div class="contest-form">
        <div class="page-header-bar">
            <h1>Edit category {{ teamCategory.name }} ({{ teamCategory.externalid }})</h1>
        </div>

        {% include 'jury/partials/team_category_form.html.twig' %}
    </div>

{% endblock %}
